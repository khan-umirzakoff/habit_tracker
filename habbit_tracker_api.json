  "components": {
    "schemas": {
      "AuthUser": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "email": {
            "type": "string",
            "format": "email",
            "maxLength": 254
          },
          "first_name": {
            "type": "string",
            "maxLength": 150
          },
          "last_name": {
            "type": "string",
            "maxLength": 150
          },
          "photo": {
            "type": "string",
            "format": "uri",
            "maxLength": 200
          }
        },
        "required": [
          "email",
          "id"
        ]
      },
      "Banner": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "maxLength": 255
          },
          "image": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": [
          "image",
          "title"
        ]
      },
      "ColorEnum": {
        "enum": [
          "light-green",
          "orange",
          "red",
          "light-blue",
          "light-gray",
          "purple",
          "pink"
        ],
        "type": "string",
        "description": "* `light-green` - Light Green\n* `orange` - Orange\n* `red` - Red\n* `light-blue` - Light Blue\n* `light-gray` - Light Gray\n* `purple` - Purple\n* `pink` - Pink"
      },
      "GoogleAuth": {
        "type": "object",
        "properties": {
          "id_token": {
            "type": "string"
          }
        },
        "required": [
          "id_token"
        ]
      },
      "Habbit": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "title": {
            "type": "string",
            "maxLength": 255
          },
          "duration": {
            "type": "integer",
            "maximum": 2147483647,
            "minimum": 0
          },
          "from_date": {
            "type": "string",
            "format": "date"
          },
          "to_date": {
            "type": "string",
            "format": "date"
          },
          "daily_statuses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HabbitDailyStatusList"
            },
            "readOnly": true
          },
          "percentage_is_completed": {
            "type": "number",
            "format": "double",
            "readOnly": true,
            "default": 0.0
          },
          "successful_days": {
            "type": "integer",
            "readOnly": true,
            "default": 0
          },
          "failed_days": {
            "type": "integer",
            "readOnly": true,
            "default": 0
          },
          "half_failed_days": {
            "type": "integer",
            "readOnly": true,
            "default": 0
          },
          "remaining_days": {
            "type": "integer",
            "readOnly": true,
            "default": 0
          },
          "passed_days": {
            "type": "integer",
            "readOnly": true,
            "default": 0
          },
          "color": {
            "allOf": [
              {
                "$ref": "#/components/schemas/ColorEnum"
              }
            ],
            "description": "Choices: light-green=Light Green, orange=Orange, red=Red, light-blue=Light Blue, light-gray=Light Gray, purple=Purple, pink=Pink.\n\n* `light-green` - Light Green\n* `orange` - Orange\n* `red` - Red\n* `light-blue` - Light Blue\n* `light-gray` - Light Gray\n* `purple` - Purple\n* `pink` - Pink"
          },
          "icon": {
            "allOf": [
              {
                "$ref": "#/components/schemas/IconEnum"
              }
            ],
            "description": "Choices: work=Briefcase (Work), music=Music note (Music), heart=Heart (Love/Health), energy=Lightning (Energy/Power), navigation=Compass (Navigation), translate=Translate / Language, fashion=T-shirt (Clothing/Fashion), video=Video camera (Video), youtube=YouTube / Play, sports=Sports ball (Sports), people_add=Person + (Add contact/People), browser=Browser window (Web), driving=Car (Driving).\n\n* `work` - Briefcase (Work)\n* `music` - Music note (Music)\n* `heart` - Heart (Love/Health)\n* `energy` - Lightning (Energy/Power)\n* `navigation` - Compass (Navigation)\n* `translate` - Translate / Language\n* `fashion` - T-shirt (Clothing/Fashion)\n* `video` - Video camera (Video)\n* `youtube` - YouTube / Play\n* `sports` - Sports ball (Sports)\n* `people_add` - Person + (Add contact/People)\n* `browser` - Browser window (Web)\n* `driving` - Car (Driving)"
          }
        },
        "required": [
          "color",
          "daily_statuses",
          "failed_days",
          "from_date",
          "half_failed_days",
          "icon",
          "id",
          "passed_days",
          "percentage_is_completed",
          "remaining_days",
          "successful_days",
          "title"
        ]
      },
      "HabbitDailyStatus": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "habbit": {
            "type": "integer",
            "readOnly": true
          },
          "status": {
            "allOf": [
              {
                "$ref": "#/components/schemas/StatusEnum"
              }
            ],
            "description": "Choices: -1=Failed, 0=Pending, 1=Half Finished, 2=Finished.\n\n* `-1` - Failed\n* `0` - Pending\n* `1` - Half Finished\n* `2` - Finished"
          },
          "date": {
            "type": "string",
            "format": "date",
            "readOnly": true
          }
        },
        "required": [
          "date",
          "habbit",
          "id",
          "status"
        ]
      },
      "HabbitDailyStatusList": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "status": {
            "allOf": [
              {
                "$ref": "#/components/schemas/StatusEnum"
              }
            ],
            "description": "Choices: -1=Failed, 0=Pending, 1=Half Finished, 2=Finished.\n\n* `-1` - Failed\n* `0` - Pending\n* `1` - Half Finished\n* `2` - Finished"
          },
          "date": {
            "type": "string",
            "format": "date"
          }
        },
        "required": [
          "date",
          "id",
          "status"
        ]
      },
      "HabbitProgress": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "total_habbits": {
            "type": "integer"
          },
          "completed_habbits": {
            "type": "integer"
          },
          "completion_percent": {
            "type": "number",
            "format": "double"
          }
        },
        "required": [
          "completed_habbits",
          "completion_percent",
          "date",
          "total_habbits"
        ]
      },
      "IconEnum": {
        "enum": [
          "work",
          "music",
          "heart",
          "energy",
          "navigation",
          "translate",
          "fashion",
          "video",
          "youtube",
          "sports",
          "people_add",
          "browser",
          "driving"
        ],
        "type": "string",
        "description": "* `work` - Briefcase (Work)\n* `music` - Music note (Music)\n* `heart` - Heart (Love/Health)\n* `energy` - Lightning (Energy/Power)\n* `navigation` - Compass (Navigation)\n* `translate` - Translate / Language\n* `fashion` - T-shirt (Clothing/Fashion)\n* `video` - Video camera (Video)\n* `youtube` - YouTube / Play\n* `sports` - Sports ball (Sports)\n* `people_add` - Person + (Add contact/People)\n* `browser` - Browser window (Web)\n* `driving` - Car (Driving)"
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string",
            "writeOnly": true
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "PatchedHabbit": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "title": {
            "type": "string",
            "maxLength": 255
          },
          "duration": {
            "type": "integer",
            "maximum": 2147483647,
            "minimum": 0
          },
          "from_date": {
            "type": "string",
            "format": "date"
          },
          "to_date": {
            "type": "string",
            "format": "date"
          },
          "daily_statuses": {
